{
  "workbench.colorTheme": "Dark+ (default dark)",
  "files.exclude": [ "_esy", "node_modules", ".git" ],
  "editor.detectIndentation": true,
  "editor.fontFamily": "Operator Mono Lig for Powerline",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.largeFileOptimizations": true,
  "editor.highlightActiveIndentGuide": true,
  "editor.indentSize": 2,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": true,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": true,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.scale": 2,
  "editor.minimap.size": "fit",
  "editor.renderIndentGuides": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.tabSize": 2,
  "editor.zenMode.hideTabs": true,
  "editor.zenMode.singleFile": true,
  "workbench.activityBar.visible": true,
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "vs-seti",
  "workbench.sideBar.visible": true,
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 1,
  "vim.useSystemClipboard": false,
  "editor.quickSuggestionsDelay": 400,

  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,

  "window.zoomLevel": 0,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": 200,
  "terminal.integrated.fontWeightBold": 500,

  "workbench.colorCustomizations": {
    "statusBar.background": "#165686",
    "statusBar.noFolderBackground": "#165686",
    "statusBar.debuggingBackground": "#165686",
    "terminal.ansiBlack": "#000",
    "terminal.ansiBlue": "#039efc",
    "terminal.ansiBrightBlue": "#68a0df",
    "terminal.ansiBrightBlack": "#686868",
    "terminal.ansiRed": "#b81400",
    "terminal.ansiBrightRed": "#ff6e67",
    "terminal.ansiGreen": "#8fac00",
    "terminal.ansiBrightGreen": "#82c100",
    "terminal.ansiYellow": "#c7c400",
    "terminal.ansiBrightYellow": "#ddd060",
    "terminal.ansiMagenta": "#ca30c7",
    "terminal.ansiBrightMagenta": "#ff77ff",
    "terminal.ansiCyan": "#00c5c7",
    "terminal.ansiBrightCyan": "#60fdff",
    "terminal.ansiWhite": "#c7c7c7",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "workbench.editor.enablePreview": false,
  "workbench.list.openMode": "doubleClick",
  "workbench.statusBar.feedback.visible": false,
  "workbench.startupEditor": "newUntitledFile",

  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    { "language": "html", "autoFix": true },
    "javascript",
    "javascriptreact"
  ],
  "eslint.options": {
    "rules": {
      "no-debugger": "off",
      "mocha/no-skipped-tests": "off"
    }
  },

  "git.autofetch": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.gitExplorer.location": "scm",
  "gitlens.historyExplorer.location": "scm",
  "gitlens.resultsExplorer.location": "scm",

  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "rgba(80,80,80,0.7)",
  "vim.easymotionMarkerForegroundColorOneChar": "white",
  "vim.easymotionMarkerForegroundColorTwoChar": "white",
  "vim.easymotionMarkerFontSize": "12",
  "vim.easymotionMarkerYOffset": 16,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#FF0000",
  "vim.highlightedyank.duration": 600,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "i"],
      "commands": ["editor.action.showHover"]
    }
  ],
  "vim.insertModeKeyBindings": [
      {
        "before": ["k", "k"],
        "after": ["<esc>"]
      },
      {
        "before": ["j", "j"],
        "after": ["<esc>"]
      }
  ],
  "vim.statusBarColorControl": false,
  "vim.statusBarColors": {
    "normal": "#165686",
    "insert": "#457C15",
    "visual": "#840E3D",
    "visualline": "#940E2D",
    "visualblock": "#A3BE8C",
    "replace": "#D08770"
  },
  "git.confirmSync": false,
  "editor.wordWrapColumn": 150,
  "editor.formatOnPaste": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "workbench.fontAliasing": "auto",
  "workbench.editor.openPositioning": "last",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": false,
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.views.compare.location": "scm",
  "gitlens.views.search.location": "scm",
  "liveshare.featureSet": "stable",
  "terminal.integrated.fontFamily": "Operator Mono Lig for Powerline",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.editor.showTabs": false,
  "vim.visualModeKeyBindingsNonRecursive": [],
  "bracketPairColorizer.consecutivePairColors": [
    "()",
    "[]",
    "{}",
    "<>",
    [
      "#7c6f64",
      "#a89984",
      "#d5c4a1",
      "#8ec07c",
      "#83a598",
      "#d3869b",
      "#fb4934",
      "#fe8019",
      "#fabd2f",
      "#b8bb26"
    ],
    "#cc241d"
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
}

